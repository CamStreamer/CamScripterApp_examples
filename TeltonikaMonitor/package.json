{
    "name": "teltonika_monitor",
    "version": "1.0.0",
    "description": "",
    "prettier": "@camstreamer/prettier-config",
    "main": "src/main.ts",
    "scripts": {
        "in:prod": "npm install --omit dev",
        "in:all": "npm install",
        "clean": "rimraf dist/* html/*",
        "try": "ts-node src/main.ts",
        "start": "cd react && npm run start && cd ..",
        "pretty": "prettier --write \"./{src,react/src,react/src/components}/*.{ts,tsx}\"",
        "build:BE": "tsc",
        "build:FE": "cd react && npm run build && cd ..",
        "build": "npm-run-all build:BE build:FE",
        "prepare-zip": "npm-run-all build in:prod",
        "zip:package": "node node_modules/camstreamerlib/CreatePackage -i -e=react,testing",
        "zip": "npm-run-all clean prepare-zip zip:package in:all"
    },
    "author": "CamStreamer s.r.o.",
    "license": "ISC",
    "dependencies": {
        "camstreamerlib": "^2.0.0"
    },
    "devDependencies": {
        "@camstreamer/prettier-config": "^2.0.4",
        "@types/node": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "eslint": "^8.47.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    }
}

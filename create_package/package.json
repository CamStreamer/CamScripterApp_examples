    "@camstreamer/prettier-config": "^2.0.4",
    "@types/node": "^18.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0"
  }

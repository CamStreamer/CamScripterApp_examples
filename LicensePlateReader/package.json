{
  "name": "LicensePlateReader",
  "version": "2.0.0",
  "description": "",
  "prettier": "@camstreamer/prettier-config",
  "main": "dist/main.ts",
  "scripts": {
    "clean": "rimraf dist/*; rm -f LicensePlateReader.zip",
    "build": "npm-run-all clean tsc",
    "tsc": "tsc",
    "pretty": "prettier --write \"./{src,html}/*.{ts,tsx,js,jsx}\"",
    "pretty:check": "prettier --check \"./{src,html}/*.{ts,tsx,js,jsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "in:prod": "npm install -production",
    "in:all": "npm install",
    "zip:package": "node node_modules/camstreamerlib/CreatePackage -i",
    "zip": "npm-run-all build in:prod zip:package in:all"
  },
  "author": "CamStreamer s.r.o",
  "license": "ISC",
  "dependencies": {
    "camstreamerlib": "^1.9.1"
  },
  "devDependencies": {
    "@camstreamer/prettier-config": "^2.0.4",
    "@types/node": "^18.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "rimraf": "^3.0.2"
  }
}
